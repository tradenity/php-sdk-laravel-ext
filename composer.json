{
  "name": "tradenity/laravel-ext",
  "version":"0.1.0",
  "description": "PHP interface to the Tradenity REST API, The easiest eCommerce API for web and mobile development.",
  "authors": [
    {
      "name": "Tradenity Inc.",
      "email": "tradenity@tradenity.com",
      "homepage": "http://www.tradenity.com",
      "role": "Vendor"
    }
  ],
  "support": {
    "email": "support@tradenity.com",
    "issues": "http://github.com/tradenity/php-sdk-laravel-ext/issues",
    "source": "http://github.com/tradenity/php-sdk-laravel-ext"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.3.0",
    "tradenity/php-sdk": ">=0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "phpdocumentor/phpdocumentor": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Tradenity\\SDK\\": "src/tradenity/sdk"
    }
  },
  "autoload-dev" : {
    "psr-4": {
      "Tradenity\\SDK\\Tests\\" : "tests/tradenity"
    }
  }
}